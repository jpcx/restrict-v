{
	"extends": ["tslint:latest", "tslint-config-prettier"],
	"rules": {
		"no-console": false,
		"max-classes-per-file": false,
		"object-literal-sort-keys": false,
		"interface-name": false,
		"callable-types": false,
		"max-line-length": [
			true,
			{
				"limit": 120,
				"ignore-pattern": "^import |^export {(.*?)}",
				"check-strings": true,
				"check-regex": true
			}
		]
	}
}
